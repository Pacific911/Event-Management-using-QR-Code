{
  "name": "event-management-using-qr-code",
  "version": "1.0.0",
  "description": "Event Management system using QR Code where the user registers, no QR code is to be stored. Instead, values that identify that user such as Names, Telephone number, Email address, Title, Company or Organization Name, and website name will be stored in a server and later will be stored in tiny dots that make up a QR Code",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "npm run build && nodemon src/index.js",
    "build-and-install": "npm install && npm run build",
    "build": "babel src -d dist",
    "prepare": "husky install",
    "lint": "eslint \"src/**/*.{js,css,html,json,md}\"",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint --fix --ext .js .",
    "prettier:fix": "prettier --write \"src/**/*.{js,css,html,json,md}\"",
    "make_model": "npx sequelize-cli model:generate --name",
    "make_migration": "npx sequelize-cli migration:create --name",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate_undo": " npx sequelize-cli db:migrate:undo",
    "migrate_undo_all": "npx sequelize-cli db:migrate:undo:all",
    "make_seed": " npx sequelize-cli seed:generate --name",
    "seed": " npx sequelize-cli db:seed:all",
    "seed_undo": " npx sequelize-cli db:seed:undo",
    "seed_undo_all": " npx sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pacific911/Event-Management-using-QR-Code.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pacific911/Event-Management-using-QR-Code/issues"
  },
  "homepage": "https://github.com/Pacific911/Event-Management-using-QR-Code#readme",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.3",
    "@babel/register": "^7.23.7",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "sequelize-cli": "^6.6.2"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.3",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.2.0",
    "cloudinary-build-url": "^0.2.4",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.36.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "joi": "^17.12.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-stub": "^1.1.1",
    "pg": "^8.11.3",
    "prettier": "^3.2.5",
    "qrcode": "^1.5.3",
    "redis": "^4.6.13",
